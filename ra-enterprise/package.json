{
    "name": "@react-admin/ra-enterprise",
    "version": "4.1.2",
    "description": "Alternative ready-to-use components for the react-admin enterprise edition. Use them to replace the open-source components you are familiar with such as the <Admin> or the <Layout>.",
    "files": [
        "*.md",
        "lib",
        "esm",
        "src"
    ],
    "author": "Marmelab <contact@marmelab.com> (https://marmelab.com/)",
    "main": "lib/src/index",
    "module": "esm/src/index.js",
    "types": "esm/src/index.d.ts",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015"
    },
    "dependencies": {
        "@react-admin/ra-editable-datagrid": "^1.0.0",
        "@react-admin/ra-form-layout": "^1.0.0",
        "@react-admin/ra-markdown": "^1.0.0",
        "@react-admin/ra-navigation": "^2.0.0",
        "@react-admin/ra-preferences": "^1.0.0",
        "@react-admin/ra-realtime": "^1.0.0",
        "@react-admin/ra-search": "^2.2.0",
        "@react-admin/ra-relationships": "^2.0.0",
        "@react-admin/ra-tour": "^1.0.0",
        "@react-admin/ra-tree": "^1.0.0",
        "@react-admin/ra-audit-log": "^2.0.2"
    },
    "devDependencies": {
        "ra-data-fakerest": "^3.9.3",
        "ra-test": "^3.14.1",
        "react": "^16.13.1",
        "react-admin": "^3.14.1",
        "react-dom": "^16.13.1"
    },
    "peerDependencies": {
        "lodash": "^4.17.20",
        "react": "^16.13.1",
        "react-admin": "^3.11.0",
        "react-dom": "^16.13.1"
    }
}
