    "devDependencies": {
        "ra-data-fakerest": "^3.9.3",
        "ra-test": "^3.14.1",
        "react-admin": "^3.14.1",
        "cross-env": "^5.2.0",
        "rimraf": "^2.6.2"
    },
    "peerDependencies": {
        "lodash": "^4.17.20",
        "react": "^16.13.1",
        "react-admin": "^3.10.0",
        "react-dom": "^16.13.1"
    },
    "dependencies": {
        "prop-types": "^15.6.1",
        "rc-tree": "^3.1.5"
    }
}
