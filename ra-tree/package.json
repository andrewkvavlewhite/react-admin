{
    "name": "@react-admin/ra-tree",
    "version": "1.6.3",
    "description": "Treeview component for react-admin",
    "sideEffects": false,
    "files": [
        "LICENSE",
        "*.md",
        "lib",
        "esm",
        "src"
    ],
    "author": "Marmelab <contact@marmelab.com> (https://marmelab.com/)",
    "main": "lib/src/index",
    "module": "esm/src/index.js",
    "types": "esm/src/index.d.ts",
    "scripts": {
